\def\dictdef#1#2{%
  \pushStorePrefix%
  \setStorePrefix{tarantas/data/#1}%
  \storePairs{#2}%
  \popStorePrefix%
}

\def\dictget#1#2{%
  \pushStorePrefix%
  \setStorePrefix{tarantas/data/#1}%
  \trt@macro@fullexpand{\getByKey{#2}}->T{\toks@}%
  \popStorePrefix%
  \the\toks@
}



